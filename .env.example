# Namecheap API credentials
# You can obtain these from your Namecheap account dashboard
NAMECHEAP_USERNAME=your_username_here
NAMECHEAP_API_KEY=your_api_key_here

# Environment: 'production' or 'sandbox'
# - 'production' uses the live Namecheap API and can make real purchases
# - 'sandbox' uses the Namecheap testing environment (requires sandbox API credentials)
NODE_ENV=production

# Optional: Specify a custom port for the MCP server (default: 3500)
# MCP_PORT=3500

# Environment setting
# Use 'production' for live API or leave commented for sandbox
# NODE_ENV=production

# Log level (debug, info, warn, error)
LOG_LEVEL=info 